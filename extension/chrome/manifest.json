{
    "name": "YTParty",
    "description": "YTParty - Watch YouTube with friends!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "scripting", "tabs", "activeTab"],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    }
}